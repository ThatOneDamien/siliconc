// This is a comment, it should not appear in the lexer's output
/* 
 * This is a multiline comment.
 * This should also not appear in the lexer's output.
 */

priv void printf(ubyte* fmt, ...);

pub uint main(uint argc)
{
    ubyte[3] str;
    str[0] = 'b';
    str[1] = '\n';
    str[2] = '\0';
    printf(str as ubyte*);
    return 0;
}
