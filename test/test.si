 */

priv void printf(ubyte* fmt, ...);

priv struct String
{
    ubyte* data;
    int    size;
}

pub uint main(uint argc)
{
    ubyte[argc] arg_strs;
    ulong i = 0;
    String a;
    a.data = "okay";
    while(i < argc - 1)
    {
        arg_strs[i] = i as ubyte + '0';
        i += 1;
    }
    arg_strs[argc - 1] = '\0';
    printf("%s\n", arg_strs as ubyte*); 

    return 0;
}
