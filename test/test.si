priv extern void printf(ubyte* fmt, ...);

struct Large
{
    uint[24] a;
}

void swap(Large* l1, Large* l2)
{
    *l1 <-> *l2;
}

pub uint main()
{
    Large l1, l2;
    l1.a[0] = 10;
    l2.a[0] = 15;
    printf("%d %d\n", l1.a[0], l2.a[0]);
    swap(&l1, &l2);
    printf("%d %d\n", l1.a[0], l2.a[0]);
    return 0;
}
